<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
  <ROOT type="array">
    <type>array</type>
    <el type="array">
       <hide_row>
        <TCEforms type="array">
          <label>Hide row class</label>
          <config>
            <type>check</type>
          </config>
        </TCEforms>
      </hide_row> 
        <text_center>
        <TCEforms type="array">
          <label>Text Center</label>
          <config>
            <type>check</type>
          </config>
        </TCEforms>
      </text_center>        
      <title>
        <TCEforms type="array">
          <label>Title</label>
          <config>
            <type>input</type>
            <size>48</size>
            <eval>trim</eval>
          </config>
        </TCEforms>
      </title>       
      <text>
        <TCEforms type="array">
          <label>Text or html code</label>
          <config>
            <type>text</type>
            <size>24</size>
            <rows>3</rows>
          </config>
        </TCEforms>
      </text>             
      <section_id>
        <TCEforms>
          <label>Select section</label>
          <config>
            <type>select</type>
            <items type="array">
              <numIndex index="0" type="array">
                <numIndex index="0">No style section</numIndex>
                <numIndex index="1"></numIndex>
              </numIndex>
              <numIndex index="1" type="array">
                <numIndex index="0">About sections</numIndex>
                <numIndex index="1">about</numIndex>
              </numIndex>
              <numIndex index="2" type="array">
                <numIndex index="0">Coachers section</numIndex>
                <numIndex index="1">coachers</numIndex>
              </numIndex>
              <numIndex index="3" type="array">
                <numIndex index="0">Offers section</numIndex>
                <numIndex index="1">offers</numIndex>
              </numIndex> 
              <numIndex index="4" type="array">
                <numIndex index="0">Contact section</numIndex>
                <numIndex index="1">contact</numIndex>
              </numIndex>                            
            </items>
            <maxitems>1</maxitems>
            <size>1</size>
          </config>
        </TCEforms>
      </section_id>
    </el>
  </ROOT>
</T3DataStructure>